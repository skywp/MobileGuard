<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:MyAppAttr="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="65dip"
    android:background="#11000000"
    android:clickable="true">

    <TextView
        android:id="@+id/tv_telsmssafe_listview_item_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dip"
        android:text="110"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv_telsmssafe_listview_item_model"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_telsmssafe_listview_item_number"
        android:layout_marginBottom="5dip"
        android:layout_marginLeft="5dip"
        android:text="拦截模式" />

    <ImageView
        android:id="@+id/iv_telsmssafe_listview_item_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerInParent="true"
        android:clickable="false"
        android:src="@drawable/ic_delete_btn" />

    <View
        android:layout_width="match_parent"
        android:layout_height="3dip"
        android:layout_below="@id/tv_settingcenter_autoupdate_content"
        android:background="@drawable/list_devider" />
</RelativeLayout>